@model AutoBarn.WebUI.Data.Entities.Model

@{
    ViewBag.Title = "Edit";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<div class="row">
    <div class="col s2 m2">&nbsp;</div>
<div class="col s8 m8">

    <h3>Edit services for @Model.Make.Name @Model.Name</h3>
    

    <div class="row">
        @foreach (var service in Model.Services)
        {
                @Html.Partial("_Service", service)
        }
    </div>
   

    <div>
        @Html.ActionLink("Back to List", "Index")
    </div>

</div>
    </div>